&lpi_i2s1_sck_active {
	config {
		drive-strength = <4>;
	};
};

&lpi_i2s1_ws_active {
        config {
                drive-strength = <4>;
        };
};

&lpi_i2s1_sd0_active {
        config {
                drive-strength = <4>;
        };
};

&lpi_i2s1_sd1_active {
        config {
                drive-strength = <4>;
        };
};


&q6core {
	wsa_swr_clk_data_pinctrl {
		status = "disabled";
	};
	cdc_quin_mi2s_gpios: msm_cdc_pinctrl_quin {
		compatible = "qcom,msm-cdc-pinctrl";
		pinctrl-names = "aud_active", "aud_sleep";
		pinctrl-0 = <&lpi_i2s1_sck_active &lpi_i2s1_ws_active
		&lpi_i2s1_sd0_active &lpi_i2s1_sd1_active>;
		pinctrl-1 = <&lpi_i2s1_sck_sleep &lpi_i2s1_ws_sleep
		&lpi_i2s1_sd0_sleep &lpi_i2s1_sd1_sleep>;
		qcom,lpi-gpios;
		#gpio-cells = <0>;
	};
	cdc_dmic01_pinctrl{
		status = "disabled";
	};
	cdc_dmic23_pinctrl{
		status = "disabled";
	};
};

&usb3phy_portselect_default {
	config {
		pins = "gpio140";
		bias-pull-down;
		drive-strength = <2>;
		};
};

