		qcom,voocphy_bidirect_cp_support;
		qcom,copycat_vooc_support;
		qcom,voocphy_current_default = <73>;
		qcom,svooc_cool_down_current_limit = <100 15 15 20 25 30 35 40 45 50 55 60 63 65 70 75 80 85 100>;
