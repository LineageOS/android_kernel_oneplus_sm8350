		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};

	pa_5g_therm {
		reg = <PM7325_ADC7_GPIO3_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
