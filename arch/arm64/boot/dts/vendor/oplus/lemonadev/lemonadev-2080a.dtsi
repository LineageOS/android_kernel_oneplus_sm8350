/*this is for different project dtsi*/
&qupv3_se4_i2c {
	status = "okay";
	s6sy792_19825@48 {
		compatible = "sec-s6sy792";
		reg = <0x48>;
		chip-name = "S6SY792";

		/* Interrupt && Irq-gpio */
		interrupt-parent = <&tlmm>;
		interrupts = <23 0x2008>;

		/* Power Config */
		enable2v8_gpio = <&tlmm 74 0x00>;
		//enable1v8_gpio = <&tlmm 6 0x1>;
		vdd_2v8-supply = <&L3C>;
		//vcc_1v8-supply = <&S1C>;
		vcc_1v8-supply = <&L8C>;
		vdd_2v8_volt = <3008000>;

		/* Other HW Resource */
		irq-gpio = <&tlmm 23 0x2008>;
		reset-gpio = <&tlmm 22 0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <&ts_int_active &ts_reset_active>;

		touchpanel,max-num-support = <10>;
		touchpanel,tx-rx-num = <16 36>;
		earsense,tx-rx-num = <16 36>;
		touchpanel,panel-coords = <1079 2399>;
		touchpanel,display-coords = <1079 2399>;
		touchpanel,touchmajor-limit = <32 54>;
		project_id = <19825 20854>;

		vid_len = <2>;
		panel_type = <8>;
		platform_support_project = <19825 20854>;
		platform_support_project_dir = <19825 19825>;
		platform_support_project_commandline = "qcom,mdss_dsi_samsung_amb655x_dsc_cmd";
		panel = <&dsi_samsung_amb655x_dsc_cmd>;

		/* SW Support Feature Control */
		//ear_sense_support;
		black_gesture_support;
		black_gesture_indep_support;
		sportify_aod_gesture_support;
		fw_edge_limit_support;
		charger_pump_support;
		//spurious_fingerprint_support;
		fw_update_app_support;
		smart_gesture_support;
		game_switch_support;
		//kernel_grip_support;
		fingerprint_underscreen_support;
		headset_pump_support;
		pressure_report_support;
		irq_need_dev_resume_ok;
		report_rate_white_list_support;
		temperature_detect_support;
		io-channels = <&pmk8350_vadc PM8350_ADC7_AMUX_THM3_100K_PU>;
		io-channel-names = "pm8350_hot_pocket_therm";
		oplus,skin_temp_chan = "pm8350_hot_pocket_therm";
		/*
		*Virtual Key Config:
		*1:touchpanel,button-type: <virtualkey_type  supportted_button-map>
		*virtualkey_type:
		*Type 1: using board_properties
		*Type 2: using same IC (button zone&& touch zone are seprate)
		*Type 3: using diffrent IC (button zone&& touch zone are seprate)
		*Type 4: No need of virtual key process
		*supportted_button-map:select|menu|home|back
		*
		*2:touchpanel,button-map
		*type (X1,Y1)(X2,Y2)(X3,Y3)(Width_X,Hight_Y)--Only when button-type is 0
		*/
		touchpanel,button-type = <4>;
		touchpanel.button-TRx = <0 0>;
	};
};

&oplus_p9415 {
	status = "ok";
};

&oplus_chg_wls {
	status = "ok";
	oplus,wls_chg_fw = "IDT_P9415_15W";
	oplus,wls_power_mw = <15000>;
	oplus,wls_phone_id = <0x03>;
	oplus,support_epp_plus;

	/* fod parameter*/
	oplus,fastchg-fod-enable;
	oplus,fastchg-match-q = /bits/ 8 <0x28>;
	oplus,fastchg-fod-parm = /bits/ 8 <0x85 0x69 0x9F 0x4C 0x8B 0x5B
						0xA5 0x8B 0x9C 0x0E 0x96 0x5A>;

	oplus,bpp-vol-mv = <5000>;
	oplus,epp-vol-mv = <10000>;
	oplus,epp_plus-vol-mv = <10000>;
	oplus,warp-vol-mv = <10000>;
	oplus,swarp-vol-mv = <12000>;
};

&battery_charger {
	oplus,wls_boost_soft_start;
};

&oplus_chg_gki {
	oplus,support_wls;
};
