&pm8350c_gpios {
	op_wlchg_pm8350c_gpio:op_wlchg_pm8350c {
		cp_boost_en_active: cp_boost_en_active {
			pins = "gpio1";
			function = "normal";
			bias-disable;
			power-source = <0>;
			output-high;
			qcom,drive-strength = <3>;
		};
		cp_boost_en_sleep: cp_boost_en_sleep {
			pins = "gpio1";
			function = "normal";
			bias-disable;
			power-source = <0>;
			output-low;
			qcom,drive-strength = <3>;
		};

		rx_en_sleep: rx_en_sleep {
			pins = "gpio2";
			function = "normal";
			bias-pull-up;
			power-source = <0>;
			output-high;
			qcom,drive-strength = <3>;
		};
		rx_en_active: rx_en_active {
			pins = "gpio2";
			function = "normal";
			bias-pull-down;
			power-source = <0>;
			output-low;
			qcom,drive-strength = <3>;
		};

		wrx_en_active: wrx_en_active {
			pins = "gpio6";
			function = "normal";
			bias-disable;
			power-source = <1>;
			output-high;
			qcom,drive-strength = <3>;
		};
		wrx_en_sleep: wrx_en_sleep {
			pins = "gpio6";
			function = "normal";
			bias-disable;
			power-source = <1>;
			output-low;
			qcom,drive-strength = <3>;
		};

		wrx_otg_active: wrx_otg_active {
			pins = "gpio7";
			function = "normal";
			bias-disable;
			power-source = <1>;
			output-high; //digital output, no invert
			qcom,drive-strength = <3>; //LOW strength
			//drive-push-pull;//CMOS
		};
		wrx_otg_sleep: wrx_otg_sleep {
			pins = "gpio7";
			function = "normal";
			bias-disable;
			power-source = <1>;
			output-low; //digital output, no invert
			qcom,drive-strength = <3>; //LOW strength
			//drive-push-pull;//CMOS
		};
	};

	battery_charger {
		vbus_ctrl_default: vbus_ctrl_default{
			pins = "gpio7";
			function = "normal";
			bias-pull-down;
			power-source = <1>;
			output-low;
		};
	};
};

&qupv3_se2_i2c_sleep {
	mux {
		pins = "gpio12", "gpio13";
		function = "gpio";
	};

	config {
		pins = "gpio12", "gpio13";
		drive-strength = <2>;
		bias-disable;
		input-enable;
	};
};

&pm8350b_gpios {
	batt_btb_adc_default: batt_btb_adc_default {
		pins = "gpio2";
		function = "normal";
		bias-pull-up;
		bias-high-impedance;
	};
};

&pmk8350_vadc {
	pinctrl-names = "default";
	pinctrl-0 = <&batt_btb_adc_default &gpio1_adc_default &gpio3_adc_default>;
	gpio1_v {
		reg = <PM8350_ADC7_AMUX_THM5_30K_PU>;
		label = "gpio1_v";
		qcom,ratiometric;
		qcom,hw-settle-time = <700>;
		qcom,pre-scaling = <1 1>;
	};

	gpio3_v {
		reg = <PM8350_ADC7_GPIO2_30K_PU>;
		label = "gpio3_v";
		qcom,ratiometric;
		qcom,hw-settle-time = <700>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm8350_gpios {
	/delete-node/ pm8350_rear_tof_therm_default;
	gpio1_v {
		gpio1_adc_default: gpio1_adc_default {
			pins = "gpio1"; /* GPIO 1 */
			function = "normal"; /* normal */
			//bias-pull-up;
			bias-high-impedance;
			bias-disable;
		};
	};

	gpio3_v {
		gpio3_adc_default: gpio3_adc_default {
			pins = "gpio3"; /* GPIO 3 */
			function = "normal"; /* normal */
			//bias-pull-up;
			bias-high-impedance;
			bias-disable;
		};
	};
};

