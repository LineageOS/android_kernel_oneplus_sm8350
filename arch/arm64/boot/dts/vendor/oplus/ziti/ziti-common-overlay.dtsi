&tlmm {

	rf_cable0_active: rf_cable0_active {
		mux {
			pins = "gpio129";
			function = "gpio";
		};
		config {
			pins = "gpio129";
			drive-strength = <2>;
			input-enable;
			bias-pull-up;
		};
	};

};



&pm7325_gpios {
	aboard_pin {
		hw_pins_default:hw_pins_default {
		};

		aboard_pin_gpio0_sleep:aboard_pin_gpio0_sleep {
				pins = "gpio2";
				function = "gpio";
				bias-pull-down;
				input-enable;
		};

		aboard_pin_gpio0_active:aboard_pin_gpio0_active {
				pins = "gpio2";
				function = "gpio";
				bias-pull-up;
				input-enable;
		};

		aboard_pin_gpio0_idle:aboard_pin_gpio0_idle {
				pins = "gpio2";
				function = "gpio";
				bias-disable;
				input-enable;
		};
	};
};

