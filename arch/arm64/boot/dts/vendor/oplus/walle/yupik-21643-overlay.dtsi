               trip = <&socd_trip>;
               cooling-device = <&cpu4_isolate 0 0>;
       };

       socd_cpu5 {
               trip = <&socd_trip>;
               cooling-device = <&cpu5_isolate 0 0>;
       };

       socd_cpu6 {
               trip = <&socd_trip>;
               cooling-device = <&cpu6_isolate 0 0>;
       };

       socd_cpu7 {
               trip = <&socd_trip>;
			   cooling-device = <&cpu7_isolate 0 0>;
