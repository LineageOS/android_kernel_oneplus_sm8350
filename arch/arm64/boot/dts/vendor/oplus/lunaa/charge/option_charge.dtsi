&usb_qmp_dp_phy {
	status = "disabled";
};

&usb2_phy1 {
	status = "disabled";
};

&usb0 {
	/delete-property/ extcon;
	dwc3@a600000 {
		usb-phy = <&usb2_phy0>, <&usb_nop_phy>;
		maximum-speed = "high-speed";
		dr_mode = "otg";
	};
};

&usb1 {
	status = "disabled";
	dwc3@8c00000 {
		status = "disabled";
	};
};

&usb2_phy0 {
	qcom,param-override-seq =
			<0x63 0x6c
			 0x8b 0x70
			 0x0e 0x74
			 0x03 0x78>;

	qcom,param-override-seq-host =
			<0x63 0x6c
			 0x8b 0x70
			 0x0e 0x74
			 0x03 0x78>;
};

