&spmi_glink_debug {
	status = "ok";

	/* Secondary (QUP) SPMI bus */
	spmi@1 {
		reg = <1>;
		#address-cells = <2>;
		#size-cells = <0>;

		qcom,smb1394-debug@b {
			compatible = "qcom,spmi-pmic";
			reg = <11 SPMI_USID>;
			qcom,can-sleep;
		};

		qcom,smb1394-debug@c {
			compatible = "qcom,spmi-pmic";
			reg = <12 SPMI_USID>;
			qcom,can-sleep;
		};

		qcom,smb1399-debug@d {
			compatible = "qcom,spmi-pmic";
			reg = <0xd SPMI_USID>;
			qcom,can-sleep;
		};
	};

};

&usb_qmp_dp_phy {
        status = "disabled";
};

&usb2_phy1 {
        status = "disabled";
};

&usb0 {
        /delete-property/ extcon;
        dwc3@a600000 {
                usb-phy = <&usb2_phy0>, <&usb_nop_phy>;
                maximum-speed = "high-speed";
                dr_mode = "otg";
        };
};

&usb1 {
        status = "disabled";
        dwc3@8c00000 {
                status = "disabled";
        };
};

&usb2_phy0 {
	qcom,param-override-seq =
			<0x07 0x6c	
			 0x0b 0x70
			 0x04 0x74
			 0x00 0x78>;

	qcom,param-override-seq-host =
			<0x07 0x6c	
			 0x0b 0x70
			 0x04 0x74
			 0x00 0x78>;
};

&thermal_zones {
        charger-therm-usr {
                disable-thermal-zone;
        };

        conn-therm-usr {
                disable-thermal-zone;
        };

        socd {
                disable-thermal-zone;
                /delete-node/ cooling-maps;
        };
};
