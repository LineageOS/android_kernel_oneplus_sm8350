# Alert Slider
CONFIG_IST_DOWN=m
CONFIG_IST_UP=m
CONFIG_MXM_DOWN=m
CONFIG_MXM_UP=m
CONFIG_OPLUS_TRIKEY_HALL=m
CONFIG_OPLUS_TRIKEY_MAIN=m

# Architectures
# CONFIG_ARCH_SUNXI is not set
# CONFIG_ARCH_HISI is not set
# CONFIG_ARCH_SHIMA is not set
# CONFIG_ARCH_YUPIK is not set

# Audio
CONFIG_OPLUS_FEATURE_MM_FEEDBACK=n

# Compression
CONFIG_CRYPTO_LZ4=y

# DTS
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# Display
CONFIG_DRM_OPLUS_NOTIFY=m
CONFIG_INPUT_TOUCHSCREEN=y
CONFIG_TOUCHIRQ_UPDATE_QOS=y
CONFIG_TOUCHPANEL_CUSTOM=m
CONFIG_TOUCHPANEL_NOTIFY=m
CONFIG_TOUCHPANEL_OPLUS=m
CONFIG_TOUCHPANEL_SAMSUNG=m
CONFIG_TOUCHPANEL_SAMSUNG_S6SY792=m
CONFIG_TOUCHPANEL_SYNAPTICS=m
CONFIG_TOUCHPANEL_SYNAPTICS_TCM_ONCELL=m

# Fingerprint
CONFIG_OPLUS_FINGERPRINT=y
CONFIG_OPLUS_FINGERPRINT_COMMON=m
CONFIG_OPLUS_FINGERPRINT_QCOM=y
CONFIG_OPLUS_FINGERPRINT_GOODIX_OPTICAL=m
CONFIG_OPLUS_FINGERPRINT_JIIOV_OPTICAL=m
CONFIG_OPLUS_SECURE=y
CONFIG_OPLUS_SECURE_COMMON=m
CONFIG_OPLUS_SECURE_QCOM=y

# Haptics
CONFIG_AW8697_HAPTIC=m

# Misc
CONFIG_OPLUS_FEATURE_ANDROIDBOOT=m
CONFIG_OPLUS_FEATURE_BUILDVARIANT=m
CONFIG_OPLUS_FEATURE_CDT=m
CONFIG_OPLUS_FEATURE_CHARGERPRESENT=m
CONFIG_OPLUS_FEATURE_CMDLINE=m
CONFIG_OPLUS_FEATURE_FTMMODE=m
CONFIG_OPLUS_FEATURE_OPROJECT=m
CONFIG_OPLUS_FEATURE_PROJECTINFO=m
CONFIG_OPLUS_FEATURE_SIMCARDNUM=m
CONFIG_OPLUS_DEVICE_INFO=m

# NFC
CONFIG_NFC_QTI_I2C=m

# Power
CONFIG_OPLUS_CALL_MODE_SUPPORT=y
CONFIG_OPLUS_CHECK_CHARGERID_VOLT=y
CONFIG_OPLUS_CHG=m
CONFIG_OPLUS_CHIP_SOC_NODE=y
CONFIG_OPLUS_SHIP_MODE_SUPPORT=y
CONFIG_OPLUS_SHORT_C_BATT_CHECK=y
CONFIG_OPLUS_SHORT_USERSPACE=y
CONFIG_OPLUS_SM8350_CHARGER=y
CONFIG_OPLUS_SMART_CHARGER_SUPPORT=y

# Sensors
CONFIG_OPLUS_FEATURE_FEEDBACK=m
CONFIG_OPLUS_FEATURE_SENSOR_CFG=m
CONFIG_OPLUS_SENSOR_FB_QC=n

# Stability
CONFIG_OPLUS_BUG_STABILITY=n

# UFS
CONFIG_OPLUS_FEATURE_UFS=y
CONFIG_SCSI_UFS_BSG=y

# VM
CONFIG_HVC_HAVEN=n

# WiFi
CONFIG_CNSS_QCA6490=y
CONFIG_OPLUS_FEATURE_WIFI_BDF=n
CONFIG_OPLUS_FEATURE_WIFI_MAC=n
CONFIG_QCA_CLD_WLAN=m

# Whitelist
CONFIG_UNUSED_KSYMS_WHITELIST="android/abi_gki_aarch64_qcom android/abi_gki_aarch64_qcom_internal android/abi_gki_aarch64_oplus_internal_whitelist android/abi_gki_aarch64_oplus_whitelist"
